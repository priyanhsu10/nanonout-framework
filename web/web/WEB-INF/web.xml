<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <filter>
        <filter-name>AppFilter</filter-name> <!-- mandatory -->
        <filter-class>app.Startup</filter-class> <!-- mandatory -->
        <init-param> <!-- optional -->
            <param-name>Startup</param-name>
            <param-value>app.Startup</param-value>
        </init-param>
    </filter>
    <filter-mapping>
        <filter-name>AppFilter</filter-name>
        <url-pattern>*</url-pattern>

    </filter-mapping>
    <servlet>
        <servlet-name>AppServlet</servlet-name>
        <servlet-class>nanonout.app.AppServlet</servlet-class>
        <load-on-startup>0</load-on-startup>
    </servlet>
</web-app>